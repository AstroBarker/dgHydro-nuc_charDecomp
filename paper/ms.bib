  archiveprefix	= {arXiv},
  arxivurl	= {http://arxiv.org/abs/1602.05956},
  author	= {{M{\"u}ller}, B. and {Heger}, A. and {Liptai}, D. and
		  {Cameron}, J.~B.},
  date-added	= {2017-08-10 14:00:59 +0000},
  date-modified	= {2017-12-05 16:36:25 +0000},
  doi		= {10.1093/mnras/stw1083},
  eprint	= {1602.05956},
  journal	= {\mnras},
  keywords	= {stars: evolution, stars: massive, supernovae: general},
  month		= jul,
  pages		= {742-764},
  primaryclass	= {astro-ph.SR},
  title		= {{A simple approach to the supernova progenitor-explosion
		  connection}},
