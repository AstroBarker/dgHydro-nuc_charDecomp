  doi		= {10.1098/rsta.2016.0271},
  eid		= {20160271},
  journal	= {Philosophical Transactions of the Royal Society of London
		  Series A},
  month		= sep,
  pages		= {20160271},
  title		= {{The mechanism(s) of core-collapse supernovae}},
  volume	= 375,
  year		= 2017,
  bdsk-file-1	= {},
  bdsk-url-1	= {https://doi.org/10.1098/rsta.2016.0271},
  bdsk-url-2	= {http://adsabs.harvard.edu/abs/2017RSPTA.37560271C}
