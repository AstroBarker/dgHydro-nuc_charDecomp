  eprint	= {1706.00072},
  journal	= {\apj},
  keywords	= {hydrodynamics, methods: analytical, methods: numerical,
		  shock waves, supernovae: general, turbulence},
  month		= mar,
  pages		= {22},
  primaryclass	= {astro-ph.HE},
  title		= {{How Turbulence Enables Core-collapse Supernova
		  Explosions}},
  volume	= 856,
  year		= 2018,
  bdsk-file-1	= {},
  bdsk-url-1	= {https://doi.org/10.3847/1538-4357/aaaec7},
