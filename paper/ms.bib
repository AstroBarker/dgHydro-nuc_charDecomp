  journal	= {\apjs},
  keywords	= {Hydrodynamics, Methods: Numerical, Relativity, Shock
		  Waves},
  month		= sep,
  pages		= {199-219},
  title		= {{The Piecewise Parabolic Method for Multidimensional
		  Relativistic Fluid Dynamics}},
  volume	= 160,
  year		= 2005,
  bdsk-file-1	= {},
  bdsk-url-1	= {http://dx.doi.org/10.1086/430905},
